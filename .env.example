# Copy to server/.env and fill in credentials

# Server
PORT=8787
# When placing this file at server/.env, use a path relative to the server folder
# so logs land in server/data (not server/server/data)
DATA_DIR=./data
TIMEZONE=America/New_York
WINDOW_DURATION_MINUTES=4
# CSV of local times in TIMEZONE for trading windows (HH:mm)
TRADING_WINDOWS=08:00,09:31,12:00,15:55

# Alpaca Paper Trading
ALPACA_KEY_ID=your_key_id
ALPACA_SECRET_KEY=your_secret_key
# Alpaca Market Data feed: 'iex' (free) or 'sip' (paid)
ALPACA_DATA_FEED=iex

# Benchmark symbol (SPY used to proxy S&P 500)
BENCHMARK_SYMBOL=SPY

# Optional: starting equity used to scale SPY when no equity samples yet
# Defaults to 100000 if not set
# START_EQUITY=100000

# MCP
ENABLE_MCP=true
MCP_SERVER_NAME=trader-bench
MCP_SERVER_DESCRIPTION=AI trading benchmark MCP server
